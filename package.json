{
  "name": "voice-chat-boilerplate",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "lint": "eslint --ext .ts,.vue .",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier --write .",
    "prepare": "nuxt prepare",
    "ws:server": "node realtime-server.js",
    "dev:all": "concurrently -k -n ws,nuxt -c cyan,magenta \"npm run ws:server\" \"npm run dev\"",
    "i18n:gen": "node scripts/generate-i18n-keys.mjs"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@pinia/nuxt": "^0.5.1",
    "nuxt": "3.11.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.38",
    "vue-router": "^4.4.5",
    "vuetify": "^3.9.5",
    "wavesurfer.js": "^7.7.12",
    "vue-i18n": "^9.10.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@nuxt/types": "^2.18.1",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-vue": "^10.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.14",
    "typescript": "5.5.4",
    "vite-plugin-vuetify": "^2.0.4",
    "vue-tsc": "^2.0.29",
    "concurrently": "^8.2.2"
  }
}
